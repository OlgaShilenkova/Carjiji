    <link rel="stylesheet" href="/styles/about.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
  </head>

  <body>
    <main>
      <section>
        <img src="<%= car.car_photo_url %>" alt="image for car " />
      </section>
      <section>
        <header>
          <h1>
            <%= car.car_year %> ðŸ¥” <%= car.car_make %> <%= car.car_model %>
          </h1>
        </header>
        <section>
          <ul>
            <li>
              <dl>
                <dt>Year:</dt>
                <dd><%= car.car_year %></dd>
              </dl>
            </li>
            <li>
              <dl>
                <dt>Make:</dt>
                <dd><%= car.car_make %></dd>
              </dl>
            </li>
            <li>
              <dl>
                <dt>Model:</dt>
                <dd><%= car.car_model %></dd>
              </dl>
            </li>
            <li>
              <dl>
                <dt>Price:</dt>
                <dd>$<%= car.listing_price/100 %></dd>
              </dl>
            </li>
            <li>
              <dl>
                <dt>Description:</dt>
                <dd> <%= car.description %> </dd>
              </dl>
            </li>
          </ul>

        </section>
      </section>
    </main>
  </body>
</html>
